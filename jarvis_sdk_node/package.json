{
  "name": "@indykiteone/jarvis-sdk-node",
  "version": "0.1.20",
  "description": "IndyKite Identity Platform JavaScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "ts-node src/example.ts",
    "clean": "rimraf dist",
    "check-es": "es-check es6 ./dist/*.js --verbose",
    "prebuild": "ts-node scripts/copyPackageVersionNumber.ts",
    "build": "tsc",
    "lint": "eslint . --fix --ext .ts",
    "prettier": "prettier --write 'src/**/*.ts'",
    "precommit": "npm run lint && npm run prettier && npm run test",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "coverage": "jest --coverage --silent",
    "coverage-ci": "jest --ci --silent --coverage --coverageReporters=json",
    "coverage-html": "jest --coverage --silent --coverageReporters=html",
    "buf-generate": "buf generate --template ../buf.gen.yaml buf.build/indykite/indykiteapis -o .."
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@types/argparse": "2.0.10",
    "@types/google-protobuf": "3.15.6",
    "@types/jest": "29.2.3",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.11.9",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "es-check": "7.0.1",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.5",
    "eslint-plugin-prettier": "4.2.1",
    "grpc_tools_node_protoc_ts": "5.3.2",
    "grpc-tools": "1.11.3",
    "jest": "29.3.1",
    "prettier": "2.8.0",
    "protoc-gen-ts": "0.8.5",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@protobuf-ts/runtime-rpc": "2.8.2",
    "@protobuf-ts/runtime": "2.8.2",
    "google-protobuf": "3.21.2",
    "jose": "^4.6.0",
    "protobufjs": "^7.0.0",
    "uuid": "^9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/indykite/jarvis-sdk-node.git"
  },
  "keywords": [
    "authentication"
  ],
  "author": "Jarvis <jarvis-support@indykite.com> (https://indykite.com/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/indykite/jarvis-sdk-node/issues"
  },
  "homepage": "https://github.com/indykite/jarvis-sdk-node#readme"
}
